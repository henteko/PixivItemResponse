{
    "name": "pixivアイテムレスポンス",
    "version": "2.0",
	"manifest_version": 2,
    "description": "pixivの作品にアイテムを送れます",
    "icons": {
        "128": "icon128.png"
    },
    "content_scripts":[{
        "matches": ["http://www.pixiv.net/*"],
        "css": ["main.css"],
        "js": ["jquery-1.7.1.min.js", "main.js"],
        "run_at": "document_end"
    }]
}